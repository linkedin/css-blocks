{
  "name": "@css-blocks/jsx",
  "version": "0.17.0",
  "description": "Statically analyzes JSX files for CSS Blocks use.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "pretest": "npm run compile && npm run lint",
    "test": "mocha dist/test --recursive --opts test/mocha.opts && npm run lint",
    "compile": "rm -rf dist && tsc --jsx preserve -p tsconfig.json",
    "prepublish": "npm run compile && tslint --project tsconfig.json -c tslint.release.json",
    "watch": "watch 'npm run test' './src' './test' --wait=3",
    "lint": "tslint --project tsconfig.json",
    "lintfix": "tslint --project tsconfig.json --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/css-blocks/css-blocks.git"
  },
  "keywords": [
    "jsx",
    "preact",
    "react",
    "static",
    "analysis",
    "CSS Blocks"
  ],
  "author": "Adam Miller <ammiller@linkedin.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/css-blocks/css-blocks/issues"
  },
  "homepage": "https://github.com/css-blocks/css-blocks/tree/master/packages/jsx#readme",
  "devDependencies": {
    "@css-blocks/code-style": "^0.17.0",
    "@css-blocks/runtime": "^0.17.0",
    "@types/babel-core": "^6.25.2",
    "@types/babel-generator": "^6.25.0",
    "@types/babel-template": "^6.25.0",
    "@types/babel-traverse": "^6.25.2",
    "@types/babel-types": "^6.7.16",
    "@types/babylon": "^6.16.1",
    "@types/chai": "^3.5.2",
    "@types/debug": "0.0.29",
    "@types/minimatch": "^2.0.29",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.18",
    "@types/prettier": "^1.8.0",
    "babel-core": "^6.25.0",
    "chai": "^3.5.0",
    "grunt": "^1.0.1",
    "grunt-release": "^0.12.0",
    "loader-utils": "^1.1.0",
    "mocha": "^3.4.1",
    "mocha-typescript": "^1.1.4",
    "mock-fs": "^4.3.0",
    "mock-require": "^2.0.2",
    "obj-str": "^1.0.0",
    "postcss": "^6.0.9",
    "preact": "^8.2.1",
    "prettier": "^1.8.2",
    "shelljs": "^0.7.8",
    "test-console": "^1.1.0",
    "ts-node": "^3.0.4",
    "tslint": "^5.9.1",
    "watch": "^1.0.2"
  },
  "peerDependencies": {
    "@css-blocks/runtime": "^0.17.0"
  },
  "dependencies": {
    "@opticss/template-api": "^0.2.0",
    "@opticss/util": "^0.1.1",
    "babel-traverse": "^6.24.1",
    "babel-types": "^6.24.1",
    "babylon": "^6.17.4",
    "css-blocks": "^0.17.0",
    "debug": "^2.6.8",
    "minimatch": "^3.0.4",
    "object.values": "^1.0.4",
    "opticss": "^0.2.0",
    "typescript": "^2.3.4"
  }
}
