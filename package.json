    "@types/babel-traverse": "^6.25.2",
    "@types/babel-types": "^7.0.0",
    "@types/babylon": "^6.16.1",
    "@types/chai": "^3.5.2",
    "@types/debug": "0.0.29",
    "@types/loader-utils": "^1.1.3",
    "@types/minimatch": "^2.0.29",
    "@types/mocha": "^2.2.41",
